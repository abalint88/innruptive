{
	"name": "inruptive-boilerplate",
	"version": "1.0.0",
	"description": "Innruptive react build",
	"main": "src/Components/App.js",
	"scripts": {
		"format": "prettier --write \"src/**/*.{js,jsx,css,scss,json} \"",
		"format:check": "prettier --list-different \"src/**/*.{js,jsx,css,scss,json} \"",
		"lint": "eslint \"src/*.{js,jsx}\"",
		"lint:fix": "eslint --fix \"src/*.{js,jsx}\"",
		"dev": "parcel src/indexDev.html --no-cache",
		"prebuild": "rmdir ./prod",
		"build": "parcel build src/index.html --experimental-scope-hoisting -d ./prod --public-url https://innruptive.com/testing/ "
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:abalint88/innruptive.git"
	},
	"keywords": [],
	"author": "Balint Apro",
	"license": "MIT",
	"homepage": "https://innruptive.com/",
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-react": "^7.6.3",
		"@emotion/core": "^10.0.22",
		"@fortawesome/fontawesome-svg-core": "^1.2.25",
		"@fortawesome/free-solid-svg-icons": "^5.11.2",
		"@fortawesome/react-fontawesome": "^0.1.7",
		"autoprefixer": "^9.7.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.3",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"grid-simpleflex": "^2.0.0",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-bundle-visualiser": "^1.2.0",
		"prettier": "^1.18.2",
		"sass": "^1.23.1"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^5.11.2",
		"axios": "^0.19.0",
		"babel-polyfill": "^6.26.0",
		"dompurify": "^2.0.7",
		"react": "^16.11.0",
		"react-app-polyfill": "^1.0.4",
		"react-dom": "^16.11.0",
		"react-render-html": "^0.6.0",
		"react-router-dom": "^5.1.2",
		"react-router-prop-types": "^1.0.4",
		"react-spinners": "^0.6.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"ie 11"
		],
		"development": [
			"last 2 chrome version",
			"last 2 firefox version",
			"last 2 safari version",
			"ie 11"
		]
	}
}
