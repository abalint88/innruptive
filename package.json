{
	"name": "inruptive-boilerplate",
	"version": "1.0.0",
	"description": "Innruptive react build",
	"main": "src/App.js",
	"scripts": {
		"format": "prettier --write \"src/**/*.{js,jsx,css,scss,json} \"",
		"format:check": "prettier --list-different \"src/**/*.{js,jsx,css,scss,json} \"",
		"lint": "eslint \"src/*.{js,jsx}\"",
		"lint:fix": "eslint --fix \"src/*.{js,jsx}\"",
		"dev": "parcel src/index.html --no-cache",
		"build": "parcel build src/index.html --experimental-scope-hoisting --public-url https://innruptive.com/",
		"sitemap": "node sitemap.js",
		"robots": "generate-robotstxt --config ./robots.js ./dist/robots.txt",
		"prod": "rm -rf dist && npm run build && npm run sitemap && npm run robots"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:abalint88/innruptive.git"
	},
	"keywords": [],
	"author": "Balint Apro",
	"license": "MIT",
	"homepage": "https://innruptive.com/",
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.9.0",
		"@emotion/core": "^10.0.22",
		"@fortawesome/fontawesome-svg-core": "^1.2.25",
		"@fortawesome/free-solid-svg-icons": "^5.11.2",
		"@fortawesome/react-fontawesome": "^0.1.7",
		"autoprefixer": "^9.7.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.5",
		"babel-node": "0.0.1-security",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"generate-robotstxt": "^8.0.2",
		"grid-simpleflex": "^2.0.0",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-bundle-visualiser": "^1.2.0",
		"parcel-plugin-custom-dist-structure": "^1.0.5",
		"prettier": "^1.18.2",
		"sass": "^1.23.1"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^5.11.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-render-html": "^0.6.0",
		"react-router-dom": "^5.1.2",
		"react-router-prop-types": "^1.0.4",
		"react-router-sitemap": "^1.2.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"ie 11"
		],
		"development": [
			"last 2 chrome version",
			"last 2 firefox version",
			"last 2 safari version",
			"ie 11"
		]
	},
	"customDistStructure": {
		"config": {
			".js": "js",
			".css": "css",
			"img": [
				".jpg",
				".png",
				".ico",
				".webmanifest"
			]
		},
		"options": {
			"development": true
		}
	}
}
